<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="System Integration Test Suite" verbose="1" parallel="methods" thread-count="4">
    <test name="SIT Tests">
        <groups>
            <run>
                <include name="smoke"/>
                <include name="integration"/>
            </run>
        </groups>
        <classes>
            <class name="com.example.sit.SimpleIntegrationTest"/>
            <class name="com.example.sit.UserApiClientTest"/>
        </classes>
    </test>
</suite>
